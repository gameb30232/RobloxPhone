<roblox xmlns:xmime="http://www.w3.org/2005/05/xmlmime" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.roblox.com/roblox.xsd" version="4">
    <Item class="Frame" referent="RBX1">
        <Properties>
            <bool name="Active">false</bool>
            <Vector2 name="AnchorPoint">
                <X>0.5</X>
                <Y>0.5</Y>
            </Vector2>
            <bool name="AutoLocalize">true</bool>
            <Color3 name="BackgroundColor3">
                <R>0.937254902</R>
                <G>0.921568627</G>
                <B>0.878431373</B>
            </Color3>
            <float name="BackgroundTransparency">0</float>
            <string name="Name">WalletApp</string>
            <UDim2 name="Position">
                <XS>0.5</XS>
                <XO>0</XO>
                <YS>0.5</YS>
                <YO>0</YO>
            </UDim2>
            <UDim2 name="Size">
                <XS>1</XS>
                <XO>0</XO>
                <YS>1</YS>
                <YO>0</YO>
            </UDim2>
            <bool name="Visible">false</bool>
        </Properties>
        
        <!-- Add clay texture and stroke -->
        <Item class="UIStroke">
            <Properties>
                <Color3 name="Color">
                    <R>0.7</R>
                    <G>0.68</G>
                    <B>0.65</B>
                </Color3>
                <float name="Thickness">3</float>
                <token name="LineJoinMode">1</token>
                <string name="Name">ClayStroke</string>
            </Properties>
        </Item>
        
        <!-- Screens Container -->
        <Item class="Frame" referent="RBX2">
            <Properties>
                <string name="Name">Screens</string>
                <UDim2 name="Size">
                    <XS>1</XS>
                    <XO>0</XO>
                    <YS>1</YS>
                    <YO>0</YO>
                </UDim2>
                <Color3 name="BackgroundTransparency">1</Color3>
            </Properties>
            
            <!-- Welcome Screen with Sheep -->
            <Item class="Frame" referent="RBX3">
                <Properties>
                    <string name="Name">WelcomeScreen</string>
                    <Color3 name="BackgroundColor3">
                        <R>0.937254902</R>
                        <G>0.921568627</G>
                        <B>0.878431373</B>
                    </Color3>
                    <UDim2 name="Size">
                        <XS>1</XS>
                        <XO>0</XO>
                        <YS>1</YS>
                        <YO>0</YO>
                    </UDim2>
                </Properties>
                
                <Item class="ImageLabel">
                    <Properties>
                        <string name="Name">SheepLogo</string>
                        <string name="Image">rbxassetid://YOUR_SHEEP_ASSET_ID</string>
                        <UDim2 name="Size">
                            <XS>0.5</XS>
                            <XO>0</XO>
                            <YS>0.3</YS>
                            <YO>0</YO>
                        </UDim2>
                        <UDim2 name="Position">
                            <XS>0.25</XS>
                            <XO>0</XO>
                            <YS>0.2</YS>
                            <YO>0</YO>
                        </UDim2>
                    </Properties>
                </Item>
                
                <Item class="TextButton">
                    <Properties>
                        <string name="Name">CreateWalletButton</string>
                        <string name="Text">Create Wallet</string>
                        <Color3 name="BackgroundColor3">
                            <R>0.180392157</R>
                            <G>0.756862745</G>
                            <B>0.494117647</B>
                        </Color3>
                        <UDim2 name="Size">
                            <XS>0.8</XS>
                            <XO>0</XO>
                            <YS>0.1</YS>
                            <YO>0</YO>
                        </UDim2>
                        <UDim2 name="Position">
                            <XS>0.1</XS>
                            <XO>0</XO>
                            <YS>0.7</YS>
                            <YO>0</YO>
                        </UDim2>
                    </Properties>
                    <Item class="UICorner">
                        <Properties>
                            <UDim name="CornerRadius">
                                <S>0.3</S>
                                <O>0</O>
                            </UDim>
                        </Properties>
                    </Item>
                </Item>
            </Item>
            
            <!-- Main Wallet Screen -->
            <Item class="Frame" referent="RBX5">
                <Properties>
                    <string name="Name">MainScreen</string>
                    <Color3 name="BackgroundColor3">
                        <R>0.937254902</R>
                        <G>0.921568627</G>
                        <B>0.878431373</B>
                    </Color3>
                    <UDim2 name="Size">
                        <XS>1</XS>
                        <XO>0</XO>
                        <YS>1</YS>
                        <YO>0</YO>
                    </UDim2>
                </Properties>
                
                <!-- Balance Display -->
                <Item class="Frame" name="BalanceCard">
                    <Properties>
                        <Color3 name="BackgroundColor3">
                            <R>0.180392157</R>
                            <G>0.756862745</G>
                            <B>0.494117647</B>
                        </Color3>
                        <UDim2 name="Size">
                            <XS>0.9</XS>
                            <XO>0</XO>
                            <YS>0.2</YS>
                            <YO>0</YO>
                        </UDim2>
                        <UDim2 name="Position">
                            <XS>0.05</XS>
                            <XO>0</XO>
                            <YS>0.1</YS>
                            <YO>0</YO>
                        </UDim2>
                    </Properties>
                    <Item class="UICorner">
                        <Properties>
                            <UDim name="CornerRadius">
                                <S>0.2</S>
                                <O>0</O>
                            </UDim>
                        </Properties>
                    </Item>
                </Item>
                
                <!-- Action Buttons -->
                <Item class="Frame">
                    <Properties>
                        <string name="Name">ActionButtons</string>
                        <Color3 name="BackgroundTransparency">1</Color3>
                        <UDim2 name="Size">
                            <XS>0.9</XS>
                            <XO>0</XO>
                            <YS>0.3</YS>
                            <YO>0</YO>
                        </UDim2>
                        <UDim2 name="Position">
                            <XS>0.05</XS>
                            <XO>0</XO>
                            <YS>0.35</YS>
                            <YO>0</YO>
                        </UDim2>
                    </Properties>
                    
                    <!-- Add UIGridLayout -->
                    <Item class="UIGridLayout">
                        <Properties>
                            <UDim2 name="CellPadding">
                                <XS>0.05</XS>
                                <XO>0</XO>
                                <YS>0.05</YS>
                                <YO>0</YO>
                            </UDim2>
                            <UDim2 name="CellSize">
                                <XS>0.45</XS>
                                <XO>0</XO>
                                <YS>0.4</YS>
                                <YO>0</YO>
                            </UDim2>
                            <token name="FillDirection">0</token>
                            <int name="FillDirectionMaxCells">2</int>
                            <token name="HorizontalAlignment">1</token>
                            <token name="VerticalAlignment">1</token>
                        </Properties>
                    </Item>
                    
                    <!-- Send Button -->
                    <Item class="TextButton" name="SendButton">
                        <Properties>
                            <string name="Name">SendButton</string>
                            <string name="Text">Send</string>
                            <Color3 name="BackgroundColor3">
                                <R>0.152941</R>
                                <G>0.152941</G>
                                <B>0.160784</B>
                            </Color3>
                            <Color3 name="TextColor3">
                                <R>0.891667</R>
                                <G>0.87309</G>
                                <B>0.87309</B>
                            </Color3>
                            <UDim2 name="Size">
                                <XS>1</XS>
                                <XO>0</XO>
                                <YS>1</YS>
                                <YO>0</YO>
                            </UDim2>
                        </Properties>
                        <Item class="UICorner">
                            <Properties>
                                <UDim name="CornerRadius">
                                    <S>0.2</S>
                                    <O>0</O>
                                </UDim>
                            </Properties>
                        </Item>
                    </Item>
                    
                    <!-- Receive Button -->
                    <Item class="TextButton">
                        <Properties>
                            <string name="Name">ReceiveButton</string>
                            <string name="Text">Receive</string>
                            <Color3 name="BackgroundColor3">
                                <R>0.180392157</R>
                                <G>0.756862745</G>
                                <B>0.494117647</B>
                            </Color3>
                            <UDim2 name="Size">
                                <XS>1</XS>
                                <XO>0</XO>
                                <YS>1</YS>
                                <YO>0</YO>
                            </UDim2>
                        </Properties>
                        <Item class="UICorner">
                            <Properties>
                                <UDim name="CornerRadius">
                                    <S>0.2</S>
                                    <O>0</O>
                                </UDim>
                            </Properties>
                        </Item>
                    </Item>
                </Item>
            </Item>
        </Item>
    </Item>
</roblox>